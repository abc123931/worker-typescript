{
  "name": "worker-typescript-template",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "private": true,
  "scripts": {
    "dev": "miniflare ./dist/worker.js",
    "build": "yarn prisma:generate && node build.js",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "jest --config jestconfig.json --verbose",
    "prisma:migrate": "DATABASE_URL=\"$(grep MIGRATE_DATABASE_URL .env | cut -d '=' -f2 | cut -d '\"' -f2)\" prisma migrate dev --preview-feature",
    "prisma:generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "esbuild": "^0.14.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "miniflare": "^1.4.1",
    "prettier": "^2.3.0",
    "prisma": "3.6.0",
    "service-worker-mock": "^2.0.5",
    "typescript": "^4.3.2",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@prisma/client": "3.6.0"
  }
}
